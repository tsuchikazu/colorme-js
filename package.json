{
  "name": "colorme",
  "description": "Colorme api client written in JavaScript for browser and node.js",
  "version": "0.0.3",
  "author": "tsuchikazu <huneyhunt77@gmail.com>",
  "bugs": {
    "url": "https://github.com/tsuchikazu/colorme-js/issues"
  },
  "dependencies": {
    "axios": "^0.5.0",
    "camelize": "^1.0.0",
    "object.assign": "^1.1.1"
  },
  "devDependencies": {
    "babel": "^4.3.0",
    "babelify": "^5.0.3",
    "browserify": "^8.1.3",
    "eslint": "^0.17.0",
    "espower-babel": "^1.3.0",
    "isparta": "^2.2.0",
    "mocha": "^2.1.0",
    "nock": "^0.59.1",
    "power-assert": "^0.10.2",
    "uglify-js": "^2.4.16"
  },
  "files": [
    "package.json",
    "README.md",
    "lib"
  ],
  "homepage": "https://github.com/tsuchikazu/colorme-js/",
  "keywords": [],
  "licenses": "MIT",
  "main": "lib/Colorme.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuchikazu/colorme-js.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify lib --outfile build/es6-project-boilderplate.js",
    "build-minify": "uglifyjs build/es6-project-boilderplate.js -o build/es6-project-boilderplate.min.js --source-map build/es6-project-boilderplate.min.map",
    "build-to5": "babel src --out-dir lib",
    "cover": "babel-node node_modules/.bin/isparta cover --report text --report html node_modules/.bin/_mocha",
    "lint": "eslint src test",
    "test": "mocha",
    "test-watch": "mocha --watch",
    "travis": "npm run lint && npm run test"
  }
}
